h3 {
  display: inline-block !important;
  margin-right: 10px;
}
h3 img {
  padding-left: 40px;
}
.hot_line {
  font-size: 18px;
  color: #82c411;
  display: inline-block;
}
li {
  list-style: circle;
}
h6 {
  font-size: 14px;
  font-weight: bold;
}
li,
p,
h6 {
  color: #330000 !important;
  font-family: "SimHei";
}
.phoneIcon01 {
  display: none;
  vertical-align: middle;
}
@media only screen and (max-width: 768px) {
  .phoneIcon {
    display: none;
  }
  .phoneIcon01 {
    display: block;
  }
}
.main {
  box-sizing: border-box;
  padding-top: 40px;
  padding-bottom: 30px;
}
