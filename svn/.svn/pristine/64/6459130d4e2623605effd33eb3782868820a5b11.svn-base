.main {
  box-sizing: border-box;
  padding-top: 50px;
  padding-bottom: 55px;
}
.main i {
  color: red;
}
.main h4,
.main .checkbox-inline,
.main .radio-inline {
  font-weight: bold;
  color: #390c20;
}
.eat {
  padding-left: 150px !important;
  padding-right: 150px !important;
}
.apply {
  border-bottom: 1px dashed #390c20;
  box-sizing: border-box;
  padding-bottom: 15px;
}
.apply p {
  text-indent: 20px;
  font-family: SimHei;
}
.eatNeedknow {
  border-bottom: 1px dashed #390c20;
  box-sizing: border-box;
  padding-bottom: 15px;
  padding-top: 15px;
  margin-bottom: 25px;
}
.eatNeedknow p {
  text-indent: 20px;
}
@media only screen and (max-width: 768px) {
  .eat {
    padding-left: 15px !important;
    padding-right: 15px !important;
  }
}
.text_left {
  text-align: left !important;
  padding-left: 10px;
  text-indent: 25px;
}
.submit {
  padding-top: 30px;
}
.input_error {
  display: none;
  color: red;
}
