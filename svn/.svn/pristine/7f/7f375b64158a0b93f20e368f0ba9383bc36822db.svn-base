.content_box {
  margin-top: 50px;
}
.content_box .box_right .box_1_1 {
  background: #9dc819;
}
.content_box .box_right .box_1_1 h4 {
  float: left;
  font-weight: bold;
  color: #390C20;
}
.content_box .box_right .box_1_1 p {
  float: right;
  font-size: 18px;
  margin: 10px 0px;
}
.content_box .box_right .box_1_1 p a {
  color: white;
  float: right;
  text-decoration: underline !important;
}
.content_box .box_right .funny {
  width: 130px;
  height: 100px;
  border-radius: 50%;
  border: 4px solid #dbdcdc;
  text-align: center;
}
.content_box .box_right .funny p {
  padding-top: 30px;
}
.content_box .box_right .content_p {
  padding-left: 20px;
  border-left: 4px solid #dbdcdc;
}
.content_box .list {
  height: 25px;
  border-radius: 50%;
  background-color: #dbdcdc;
  border-radius: 10px;
  overflow: hidden;
}
.content_box .logo {
  padding: 20px 0px;
}
.content_box .user_mess {
  text-align: center;
  padding: 15px 0px;
  border-bottom: 1px solid #dbdcdc;
  border-top: 1px solid #dbdcdc;
  color: #390C20;
  font-weight: bold;
}
.content_box .mess_box {
  color: gray;
  font-weight: bold;
  margin-bottom: 10px;
}
.content_box .change_mess {
  text-align: right;
}
.content_box .change_mess a {
  font-size: 120%;
  color: #390c20;
}
.content_box .change_mess a:hover {
  text-decoration: underline !important;
}
.content_box .special {
  text-align: center;
  padding: 15px 0px;
  border-bottom: 1px solid #dbdcdc;
  border-top: 1px solid #dbdcdc;
  color: #390C20;
  font-weight: bold;
}
.content_box .special_box {
  color: gray;
  font-weight: bold;
}
.content_box .change_special {
  text-align: right;
}
.content_box .change_special a {
  font-size: 120%;
  color: #390c20;
}
.content_box .change_special a:hover {
  text-decoration: underline !important;
}
.content_box .power {
  text-align: center;
  padding: 15px 0px;
  border-bottom: 1px solid #dbdcdc;
  border-top: 1px solid #dbdcdc;
  color: #390C20;
  font-weight: bold;
}
.content_box .power_ul li {
  list-style-type: circle !important;
  padding: 5px 0px;
  color: #390C20;
}
.content_box .power_ul li a {
  color: #9dc819 !important;
}
.content_box .power_ul li a:hover {
  text-decoration: underline !important;
}
.content_box .user_ma {
  text-align: center;
  padding: 15px 0px;
  border-bottom: 1px solid #dbdcdc;
  border-top: 1px solid #dbdcdc;
  color: #390C20;
  font-weight: bold;
}
.content_box .user_ma_box {
  color: gray;
  font-weight: bold;
}
.content_box .bye {
  text-align: center;
  padding: 15px 0px;
  border-bottom: 1px solid #dbdcdc;
  border-top: 1px solid #dbdcdc;
  font-weight: bold;
}
.content_box .bye a {
  color: #390C20;
}
